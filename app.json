{
  "expo": {
    "name": "vibecode",
    "slug": "vibecode",
    "owner": "les-champs-bretons",
    "scheme": "vibecode",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.leschanvriersunis.app"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-video",
      "expo-web-browser",
      "react-native-edge-to-edge"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "22cfd01a-59e3-43fa-9ba6-15688ef98af2"
      }
    }
  }
}