{
  "expo": {
    "name": "vibecode",
    "slug": "vibecode",
    "owner": "les-champs-bretons",
    "scheme": "vibecode",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "android": {
        "edgeToEdgeEnabled": true,
        "package": "com.leschanvriersunis.app",
        "permissions": [
          "INTERNET",
          "ACCESS_NETWORK_STATE",
          "READ_MEDIA_IMAGES",
          "READ_MEDIA_AUDIO"
        ],
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundImage": "./assets/adaptive-icon-background.png",
          "backgroundColor": "#FFFFFF"
        },
        "versionCode": 1,
        "googlePlayServicesVersion": "33.2.0"
      }
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-video",
      "expo-web-browser",
      "react-native-edge-to-edge"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "22cfd01a-59e3-43fa-9ba6-15688ef98af2"
      }
    },
    "android": {
      "package": "com.leschampsbretons.vibecode"
    }
  }
}
